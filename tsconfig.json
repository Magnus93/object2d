{
  "compilerOptions": {
    "allowJs": true,            
    "checkJs": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "ES6",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports" : true,
    "esModuleInterop" : true,
    "strict": true,
    "skipLibCheck": true    
  }
}